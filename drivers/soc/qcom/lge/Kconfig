menu "LGE Specific Patches"

config LGE_BOOTLOADER_LOG
	tristate "Support LGE bootloader log"
	default n
	depends on MACH_LGE
	help
	  LGE bootloader log

config LGE_EARJACK_DEBUGGER
	bool "Earjack UART debugger support"
	default n
	depends on MACH_LGE
	help
	  Support earjack type UART debugger cable

config LGE_BOOTLOADER_TIME_CHECKER
    tristate "Supprt bootloader(LK, SBL) time"
    default n
    depends on MACH_LGE
    help
      Check LK, SBL booting time at
      sys/module/lge_bootloader_time_checker/parameters/lk_time
      sys/module/lge_bootloader_time_checker/parameters/sbl_time

config LGE_HANDLE_PANIC
	tristate "Support LGE Crash Handler"
	default n
	depends on MACH_LGE
	help
	  LGE Crash Handler

config LGE_PM_DEBUG
	tristate "Support LGE PM Debug Feature"
	default n
	depends on MACH_LGE
	help
	  LGE PM Debug Feature

config LGE_MISC_CHECKER
    tristate "Support LGE misc checker"
    default n
    depends on MACH_LGE
    help
      Read/Write ftm items in misc.

config PRE_SELF_DIAGNOSIS
	tristate "Support LGE self diagnosis for factory"
	default n
	depends on MACH_LGE
	help
	  LGE self diagnosis

config CHECK_CPU
    tristate "Support LGE check cpu for factory"
    default n
    depends on MACH_LGE
    help
      LGE factory check cpu

config CHECK_EMMC
    tristate "Support LGE check emmc for factory"
    default n
    depends on MACH_LGE
    help
      LGE factory check emmc

config CHECK_RAM
    tristate "Support LGE check ram for factory"
    default n
    depends on MACH_LGE
    help
      LGE factory check ram

config CHECK_MATCHED
    tristate "Support LGE check matched for factory"
    default n
    depends on MACH_LGE
    help
      LGE factory check matched

config LGE_QSDL_SUPPORT
    tristate "Support for Quality Statistics Data Logging"
    default n
    depends on MACH_LGE
    help
       QSDL is a feature which notify infomation for quality statistics
       data logging of device.

endmenu

menu "LGE QFPROM Values"

config LGE_QFPROM_INTERFACE
	bool "Support qfprom interface"
	default n
	depends on MACH_LGE
	help
	  LGE QFPROM DRIVER.

config LGE_QFPROM_SECHASH
	string "Support qfprom sec hash"
	default 0x0
	depends on LGE_QFPROM_INTERFACE
	help
	  LGE QFPROM DRIVER-SEC partition's hash value.

config LGE_PM_UNIFIED_NODE
	bool "enable LGE PM Unified sysfs node"
	help
	  Support LGE PM Unified sysfs node

endmenu
