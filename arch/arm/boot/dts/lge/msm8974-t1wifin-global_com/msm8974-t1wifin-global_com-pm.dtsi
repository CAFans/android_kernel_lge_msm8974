&soc {
	i2c@f9923000 {
		rt9428@36 {
			status =  "ok";
		};

		bq24296@6b {
			status = "ok";
		};
	};
};

&spmi_bus {
	qcom,pm8941@0 {
		qcom,temp-alarm@2400 {
			spmi-dev-container;
			#address-cells = <1>;
			#size-cells = <1>;
			qcom,chg-bat-if@1200 {
				status = "ok";
				revision = "rev_b...";
				reg = <0x1200 0x100>;
				interrupts =	<0x0 0x12 0x0>;
				interrupt-names = "batt-pres";
			};
		};

		pm8941_chg: qcom,charger {
			revision = "...rev_a";
			status = "ok";

			qcom,bat-if@1200 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,boost@1500 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,buck@1100 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,chgr@1000 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,dc-chgpth@1400 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,chg-misc@1600 {
				revision = "...rev_a";
				status = "ok";
			};

			qcom,usb-chgpth@1300 {
				revision = "...rev_a";
				status = "ok";
			};
		};

		vadc@3100 {
			chan@14 {
				status = "ok";
			};
			chan@15 {
				status = "ok";
				revision = "rev_c...";
			};
		};

		qcom,currentnow@3800 {
			status = "ok";
		};
	};
};

