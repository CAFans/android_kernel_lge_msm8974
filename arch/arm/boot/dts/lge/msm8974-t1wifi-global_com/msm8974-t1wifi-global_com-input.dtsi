&soc {
	i2c@f9924000 {
		atmel_2954@4a {								/*touch ic I2C bus adddress*/
			status = "ok";								/*OK-used*/
			compatible = "unified_driver_ver2,mXT2954";	/*dtsi name*/
			interrupt-parent = <&msmgpio>;
			interrupts = <5 0x2>;
		};
		atmel_2954_rev_a@4a {						/*touch ic I2C bus adddress*/
			status = "ok";								/*OK-used*/
			compatible = "unified_driver_ver2,mXT2954";	/*dtsi name*/
			interrupt-parent = <&msmgpio>;
			interrupts = <5 0x2>;
		};
	};
};
